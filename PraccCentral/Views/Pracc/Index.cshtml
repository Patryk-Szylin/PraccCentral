@model PraccCentral.Models.Pracc

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<div class="row">
    <div class="col-sm-12 col-lg-8">
        @using (Html.BeginForm("Index", "Pracc", null, FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-container" }))
            {
            @Html.AntiForgeryToken()
            <h2 style="text-align: center;">Add Pracc</h2>
            <h4>@ViewBag.ResultInfo</h4>
                    @*<h4>Send your enquiry</h4>*@
                    @*<hr />*@
                    <div class="form-group">
                        @Html.LabelFor(m => m.Map, new { @class = "col-md-2 col-sm-2 control-label" })
                        <div>
                            @Html.TextBoxFor(m => m.Map, new { @class = "form-control" })
                            @Html.ValidationMessageFor(m => m.Map)
                        </div>
                    </div>
                    <div class="form-group">
                        <div>
                            <input type="submit" class="btn-send orange icon-mail" value="Send" />
                        </div>
                    </div>
        }
    </div>
</div>